<?xml version="1.0"?>
<configuration>

  <configSections>
    <section name="socketServer" type="LW.NSocket.Server.Config.SocketServerConfig, NSocket.Server"/>
  </configSections>

  <socketServer>
    <servers>
      <server name="binaryXML" port="8404" socketBufferSize="8192" messageBufferSize="8192" maxMessageSize="102400" maxConnections="20000" serviceType="XmlServer.MyService, XmlServer" protocol="asyncBinaryXMLOuter" />
    </servers>
  </socketServer>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
